
@{
    ViewBag.Title = "UnAuthorized";
}

<div class="error-page">
    <h2 class="headline text-red">401</h2>

    <div class="error-content">
        <h3><i class="fa fa-warning text-red"></i> No estas Autorizado.</h3>

        <p>
            No tienes permiso de ejecutar esta acción.<br>
            O no has iniciado sección con tu cuenta.
        </p>

        <a href="@Url.Action("Login", "Account")" class="btn btn-default" role="button">Login</a>
    </div>
</div>

