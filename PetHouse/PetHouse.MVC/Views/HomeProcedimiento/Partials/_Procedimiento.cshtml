<div style="margin-left: 15px; margin-right: 15px;">
    <div class="box">
        <div class="box-body">
            <div class="col-md-4">
                <img alt="PetHouse" src="~/Content/img/logo.png" height="100" />
            </div>
            <div class="col-md-8" style="padding-top:25px">
                <div class="form-inline">
                    @using (Html.BeginForm("PanelProcedimiento", "HomeProcedimiento", FormMethod.Post))
                    {
                        @Html.AntiForgeryToken()
                        <div class="form-group pull-right col-xs-12">
                            <div class="col-xs-9 col-md-10 text-right">
                                @Html.TextBox("ExpedienteId", null, htmlAttributes: new { @class = "form-control" })
                            </div>
                            <button type="submit" class="col-xs-3 col-md-2 btn btn-info  pull-right">IR</button>
                        </div>
                    }
                </div>
                <span class="pull-right" style="padding-right: 15px;">Ingrese el Id del expediente de la mascota a buscar</span>
            </div>
        </div>
    </div>
</div>

