@using PetHouse.MVC.Models
@{
    ViewBag.Title = "";
}

@Html.Partial("Helpers/_ViewError")

<div class="col-md-9">
    @Html.Partial("SideBars/_Ingreso")

    <div class="col-md-12">
        <div class="box">
            <div class="box-header with-border">
                <h3 class="box-title" style="font-family: 'Roboto', sans-serif; color: #6F84FF;">Lista de Expedientes</h3>
            </div>
            <div class="box-body">
                @Html.Partial("Expediente/_Index", (List<Expediente>)ViewBag.Expedientes)
            </div>
        </div>
    </div>
    <div class="col-md-12">
        <div class="box">
            <div class="box-header with-border">
                <h3 class="box-title" style="font-family: 'Roboto', sans-serif; color: #6F84FF;">Lista de Mascotas</h3>
            </div>
            <div class="box-body">
                @Html.Partial("Mascota/_Index", (List<Mascota>)ViewBag.Mascotas)
            </div>
        </div>
    </div>
</div>

<div class="col-md-3">
    @Html.Partial("SideBars/_Busqueda")
</div>